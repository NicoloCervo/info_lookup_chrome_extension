{
  "manifest_version": 3,
  "name": "Regex Matcher Extension",
  "version": "1.0",
  "description": "Matches regex patterns and adds information from local mock API",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "http://localhost:5001/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}